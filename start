class Dog

    def initialize(name, breed, age, bark, fav_foods)
        @name = name
        @breed = breed
        @age = age
        @bark = bark
        @favorite_foods = fav_foods
    end

    def name
        @name
    end
   
    def breed
        @breed
    end

    def age
        @age
    end

    def bark
       @bark
    end
    
    def fav_foods
        @favorite_foods
    end

    def age=(num)
        @age = num
    end
